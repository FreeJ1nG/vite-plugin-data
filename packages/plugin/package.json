{
  "name": "vite-plugin-data",
  "description": "Resolve and bundle data loader file exports at build-time.",
  "license": "ISC",
  "version": "0.1.1",
  "keywords": [
    "vite",
    "vite-plugin",
    "data",
    "loader",
    "json",
    "build",
    "build-time",
    "compile",
    "compile-time",
    "exports",
    "resolve"
  ],
  "scripts": {
    "build": "tsc -p src/tsconfig.build.json"
  },
  "files": [
    "lib"
  ],
  "type": "module",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    }
  },
  "peerDependencies": {
    "vite": "^5.0.0"
  },
  "dependencies": {
    "@types/picomatch": "^2.3.3",
    "picomatch": "^4.0.2",
    "really-relaxed-json": "^0.3.2",
    "ts-node": "^10.9.2",
    "zod": "^3.23.4"
  }
}